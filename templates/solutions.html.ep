<table>
  <thead>
    <tr>
      <th>Select</th>
      <th>Name</th>
      <th>Effectiveness</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
% for my $id ( keys %$solutions ) {
%   my $solution = $solutions->{$id};
    <tr>
      <td><input type=radio id="<%= $id.'.s' %>" value="<%= $id %>" name="solution" /></td>
      <td>
	<label for="<%= $id.'.s' %>">
	  <%= $id %></td>
	</label>
      <td>
	  <%= sprintf "%.02f", $solution->{efficiency} %>
      </td>
    </tr>
% }
  </tbody>
</table>
